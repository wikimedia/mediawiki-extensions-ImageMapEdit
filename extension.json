			],
			"styles": {
				"ime.css": {}
			},
			"dependencies": [
				"mediawiki.template.mustache",
				"oojs-ui",
				"oojs-ui.styles.icons-editing-advanced"
			],
			"templates": [
				"ImageMapEdit.mustache"
			],
			"messages": [
				"imagemapedit-bottomleft",
