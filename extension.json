	"license-name": "GPL-3.0-only",
	"type": "parserhook",
	"requires": {
		"MediaWiki": ">= 1.43.0"
	},
	"config_prefix": "ime",
	"config": {
		"FileTypeList": {
			"public": false,
			"value": [ "png", "gif", "jpg", "jpeg" ]
		}
	},
	"MessagesDirs": {
		"ImageMapEdit": [
			"i18n"
		]
	},
	"AutoloadNamespaces": {
		"MediaWiki\\Extension\\ImageMapEdit\\": "src/"
	},
	"ResourceModules": {
